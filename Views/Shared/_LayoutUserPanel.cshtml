
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/css/bootstrap.rtl.min.css"
          integrity="sha384-PJsj/BTMqILvmcej7ulplguok8ag4xFTPryRq8xevL7eBYSmpXKcbNVuy+P0RMgq" crossorigin="anonymous">


    <!-- Bootstrap Icon -->
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">

    <link href="~/css/acc.css" rel="stylesheet">

    <style>
        .nav-tabs .nav-item.show .nav-link,
        .nav-tabs .nav-link.active {
            color: rgb(255, 72, 0);
        }

        .nav-link {
            color: white;
        }

            .nav-link:hover {
                color: rgb(255, 72, 0);
            }
    </style>

    <title>حساب کاربری</title>
</head>

<body style="background-color: #525252;">

    <!--header start-->
    <nav class="navbar navbar-expand-lg sf-gray-primary" style="background-color: #292929;">
        <div class="container-fluid">
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent"
                    aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent" style="color: rgb(255, 72, 0);">
                <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" asp-action="Index" asp-controller="Home" style="color: rgb(255, 72, 0);">صفحه اصلی</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" aria-current="page" asp-action="Index" asp-controller="UserPanel" style="color: rgb(255, 72, 0);">داشبورد</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="AddMusic" asp-controller="UserPanel" style="color: rgb(255, 72, 0);">آپلود آهنگ</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" asp-action="UserInfo" asp-controller="UserPanel" style="color: rgb(255, 72, 0);">حساب کاربری</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-danger border rounded-3" asp-action="Logout" asp-controller="User">خروج</a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>

        @RenderBody()



    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-geWF76RCwLtnZ8qwWowPQNguL3RmwHVBC9FhGdlKrxdiJJigb/j/68SIy3Te4Bkz"
            crossorigin="anonymous"></script>

        
        </body>
</html>
